{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "azuredatafactorygroup"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « password » de « AzureSqlDatabase1 »"
		},
		"Salesforce_LinkedService_clientSecret": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « clientSecret » de « Salesforce_LinkedService »"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "basededonneesqlazure.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "basededonneesazuresql"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "basededonneesazuresql"
		},
		"Salesforce_LinkedService_properties_typeProperties_environmentUrl": {
			"type": "string",
			"defaultValue": "https://agentforce.my.salesforce.com"
		},
		"Salesforce_LinkedService_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "aboud083814@agentforce.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredatafactorygroup_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"Salesforce_LinkedService_clientSecret": {
						"value": "[parameters('Salesforce_LinkedService_clientSecret')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"Salesforce_LinkedService_properties_typeProperties_environmentUrl": {
						"value": "[parameters('Salesforce_LinkedService_properties_typeProperties_environmentUrl')]"
					},
					"Salesforce_LinkedService_properties_typeProperties_clientId": {
						"value": "[parameters('Salesforce_LinkedService_properties_typeProperties_clientId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}